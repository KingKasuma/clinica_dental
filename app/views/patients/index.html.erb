<p id="notice"><%= notice %></p>

<h1>Listing Patients</h1>

<table>
  <thead>
    <tr>
      <th>Nombres</th>
      <th>Apellido paterno</th>
      <th>Apellido materno</th>
      <th>Edad</th>
      <th>Ocupacion</th>
      <th>Procedencia</th>
      <th>Direccion</th>
      <th>Tiempo disponible</th>
      <th>Telefono</th>
      <th>Email</th>
      <th>Ci</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patients.each do |patient| %>
      <tr>
        <td><%= patient.nombres %></td>
        <td><%= patient.apellido_paterno %></td>
        <td><%= patient.apellido_materno %></td>
        <td><%= patient.edad %></td>
        <td><%= patient.ocupacion %></td>
        <td><%= patient.procedencia %></td>
        <td><%= patient.direccion %></td>
        <td><%= patient.tiempo_disponible %></td>
        <td><%= patient.telefono %></td>
        <td><%= patient.email %></td>
        <td><%= patient.CI %></td>
        <td><%= link_to 'Show', patient %></td>
        <td><%= link_to 'Edit', edit_patient_path(patient) %></td>
        <td><%= link_to 'Destroy', patient, method: :delete, data: { confirm: 'Estas seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patient', new_patient_path %>
